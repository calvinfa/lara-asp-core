{
  "name": "calvinfa/lara-asp-core",
  "homepage": "https://github.com/calvinfa/lara-asp-core",
  "description": "The Awesome Set of Packages for Laravel - The Core.",
  "readme": "README.md",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "laravel-package",
    "laravel",
    "php"
  ],
  "require": {
    "php": ">=7.4.1",
    "ext-mbstring": "*",
    "laravel/framework": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0",
    "orchestra/testbench": "^6.9.0"
  },
  "suggest": {
    "ext-fileinfo": "Required for league/flysystem"
  },
  "autoload": {
    "psr-4": {
      "LastDragon_ru\\LaraASP\\Core\\": "src/"
    },
    "exclude-from-classmap": [
      "src/**Test.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
